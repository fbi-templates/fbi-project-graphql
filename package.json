{
  "name": "fbi-project-graphql",
  "version": "0.0.10",
  "description": "GraphQL API Server template",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start pm2.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neikvon/fbi-project-graphql.git"
  },
  "keywords": [
    "graphql"
  ],
  "author": "neikvon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neikvon/fbi-project-graphql/issues"
  },
  "homepage": "https://github.com/neikvon/fbi-project-graphql#readme",
  "dependencies": {
    "graphql-yoga": "1.16.7",
    "merge-graphql-schemas": "1.5.7",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.14",
    "@types/node": "10.12.0",
    "@types/ws": "6.0.1",
    "deepmerge": "2.2.1",
    "fs-extra": "7.0.0",
    "globby": "8.0.1",
    "graphql": "^0.13.2",
    "graphql-import": "0.7.1",
    "graphql-schema-typescript": "1.2.8",
    "graphqlgen": "0.2.12",
    "nodemon": "1.18.5",
    "prisma-binding": "2.1.6",
    "prisma-generate-schema": "1.19.0",
    "rimraf": "2.6.2",
    "tslint": "5.11.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.1.3"
  },
  "fbi": {
    "template": {
      "name": "fbi-project-graphql"
    },
    "tasks": {
      "build": {
        "alias": "b",
        "desc": "Build the project for the specified environment."
      },
      "serve": {
        "alias": "s",
        "desc": "Compile the code and start the development server."
      },
      "generate": {
        "alias": "g",
        "desc": "Generate type-safe resolvers based on your GraphQL Schema in TypeScript."
      }
    }
  }
}
